{
  "version": 2,
  "builds": [
    {
      "src": "myproject/myproject/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb", "runtime": "python3.9"
      }
    },
    {
      "src": "build_file.sh",
      "use": "@vercel/static-build",

    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "myproject/myproject/wsgi.py"
    },
    {
      "src": "static/(.*)",
      "dest": "/static/$1"
    }
  ]
}